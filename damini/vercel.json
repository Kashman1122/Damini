{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "PYTHON_PATH": ".venv/Scripts/python"
  },
  "buildCommand": "if [ -d .venv ]; then . .venv/Scripts/activate; fi"
}
